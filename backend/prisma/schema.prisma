model SurfCamp {
  id       Int     @id @default(autoincrement())
  name     String
  location String
  price    Decimal
  rating   Int
  bookings Booking[]
}

model Booking {
  id        Int     @id @default(autoincrement())
  surfCamp  SurfCamp @relation(fields: [surfCampId], references: [id])
  surfCampId Int
  user      User    @relation(fields: [userId], references: [id])
  userId    Int
  startDate Date
  endDate   Date
}

model User {
  id       Int     @id @default(autoincrement())
  email    String   @unique
  password String
  bookings Booking[]
}